{
  "AWSTemplateFormatVersion": "2010-09-09",
  "Description": " Adding route from private subnet to NAT Gateway",
  "Parameters": 
  {   
    "rtbPrivate": {
      "Description": "privateroutetable",
      "Type": "String",
      "Default": "10.1.0.0/16"
    },
  },
 "Resources": 
  {  
"routePrivate": {
      "Type": "AWS::EC2::Route",
      "Properties": {
        "DestinationCidrBlock": "0.0.0.0/0",
        "RouteTableId": {"Ref": "rtbPrivate"},
        "InstanceId": {"Ref": "NAT"}
      }
}
}
